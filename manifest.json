{
  "name": "TabSaver",
  "version": "1.0",
  "description": "Save existing tabs in a state",
  "manifest_version": 2,
  "browser_action":{
    "default_popup": "UserInterface.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://cdnjs.cloudflare.com/",
    "https://stackpath.bootstrapcdn.com/"
  ],
  "content_security_policy": "script-src 'self' https://stackpath.bootstrapcdn.com/; object-src 'self'",
  "icons": {
    "128": "icon.png"
  }
}